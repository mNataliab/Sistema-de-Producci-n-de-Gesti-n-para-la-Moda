<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginPruebaController</servlet-name>
        <servlet-class>Controllers.LoginPruebaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControllers</servlet-name>
        <servlet-class>Controllers.LoginControllers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductosController</servlet-name>
        <servlet-class>Controllers.ProductosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerPedido</servlet-name>
        <servlet-class>Controllers.ControllerPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioController</servlet-name>
        <servlet-class>Controllers.UsuarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerIngreso</servlet-name>
        <servlet-class>Controllers.ControllerIngreso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerMateriales</servlet-name>
        <servlet-class>Controllers.ControllerMateriales</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerSalida</servlet-name>
        <servlet-class>Controllers.ControllerSalida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controllertpm</servlet-name>
        <servlet-class>Controllers.Controllertpm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerTipoProducto</servlet-name>
        <servlet-class>Controllers.ControllerTipoProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContraControllers</servlet-name>
        <servlet-class>Controllers.ContraControllers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginPruebaController</servlet-name>
        <url-pattern>/loginprueba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControllers</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductosController</servlet-name>
        <url-pattern>/Productos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerPedido</servlet-name>
        <url-pattern>/Cpedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioController</servlet-name>
        <url-pattern>/UsuarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerIngreso</servlet-name>
        <url-pattern>/ControllerIngreso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerMateriales</servlet-name>
        <url-pattern>/ControllerMateriales</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerSalida</servlet-name>
        <url-pattern>/ControllerSalida</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controllertpm</servlet-name>
        <url-pattern>/Ctpm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerTipoProducto</servlet-name>
        <url-pattern>/CTipoProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContraControllers</servlet-name>
        <url-pattern>/ContraControllers</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
